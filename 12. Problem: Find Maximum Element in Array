#include <stdio.h>
int main()
{
    int n, mx;
    scanf("%d", &n);

    int arr[n];
    for (int i = 0; i < n; i++)
    {
        scanf("%d", &arr[i]);
    }
    mx = arr[0];

    for (int i = 1; i < n; i++)
    {
        if (arr[i] > mx)
        {
            mx = arr[i];
        }
    }
    printf("%d\n", mx);

    return 0;
}
